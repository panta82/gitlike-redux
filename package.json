{
  "name": "gitlike-redux",
  "version": "0.0.1",
  "description": "Use redux like git",
  "scripts": {
    "clean": "rm -rf dist",
    "prepublishOnly": "npm run build",
    "build": "rollup -c",
    "test": "jest",
    "watch": "jest --watchAll",
    "lint": "eslint ./src"
  },
  "main": "dist/index.js",
  "unpkg": "dist/umd/index.js",
  "browser": "dist/umd/index.js",
  "module": "dist/es/index.js",
  "types": "dist/types/index.d.ts",
  "files": [
    "dist",
    "src"
  ],
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.18.2",
    "@rollup/plugin-babel": "^5.3.1",
    "@rollup/plugin-node-resolve": "^13.3.0",
    "@types/jest": "^27.5.1",
    "@typescript-eslint/eslint-plugin": "^5.26.0",
    "@typescript-eslint/parser": "^5.26.0",
    "eslint": "^8.16.0",
    "eslint-config-prettier": "^8.5.0",
    "import-sort-style-panta": "0.0.1",
    "jest": "^28.1.0",
    "prettier": "latest",
    "prettier-plugin-import-sort": "0.0.7",
    "redux": "^4.2.0",
    "rollup": "^2.75.0",
    "rollup-plugin-terser": "^7.0.2",
    "rollup-plugin-typescript2": "^0.31.2",
    "ts-jest": "^28.0.3",
    "typescript": "^4.7.2"
  },
  "keywords": [
    "redux",
    "reducer",
    "state",
    "git",
    "simplified",
    "simpler"
  ],
  "author": "Ivan Pantic",
  "license": "MIT",
  "importSort": {
    ".ts, .tsx": {
      "style": "panta",
      "parser": "typescript"
    },
    ".js, .jsx": {
      "style": "panta"
    }
  },
  "dependencies": {
    "@babel/runtime": "^7.18.3"
  }
}
